.addimage{
    height:200rpx;
    width:200rpx;
    background-size: 100%;
    margin:0 auto;
    margin-top:120rpx;
    overflow: hidden;
    position: relative;
    border-radius:10rpx;
}
page{
    width:100%;
    height:100%;
    position: relative;
    float:left;
}
.addimage image{
    display: block;
    width:100%;
    height:100%;
}
.groupname{
    display: block;
    width:510rpx;
    margin:0 auto;
    margin-top:120rpx;
    height:100rpx;
    border:1rpx solid #1DCE6C;
    text-align:center;
    font-size:30rpx;
}
.quanzi_no{
    border:none;
    color:#b2b2b2;
    font-size:36rpx;
    width:670rpx;
    outline: none;
    position: absolute;
    bottom:140rpx;
    left:40rpx;
}
.quanzi_no::after{
    border:none;
}
.tishi{
    position: absolute;
    width:100%;
    display: block;
    text-align:center;
    bottom:74rpx;
    left:0;
    color:#777;
    font-size:26rpx;
}
#quanzi_yes{
    background:#1DCE6C;
    color:#fff;
}